   "source": [
    "from collections import Counter\n",
    "def DiffAdjacent(A):\n",
    "    count=Counter(A).most_common()\n",
    "    N=len(A)\n",
    "    if count[0][1]>(N+1)//2:\n",
    "        return False\n",
    "    else:\n",
    "        return True"
   ]
